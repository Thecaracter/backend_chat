{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/src/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/src/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/src/$1"
        }
    ],
    "env": {
        "PORT": "3000"
    }
}